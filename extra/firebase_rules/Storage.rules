rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
  	match /anon/{imageId} {
    	allow read, write: if request.auth == null;
    }
    match /images/{userId} {
    	allow read, write: if request.auth != null && request.auth.uid == userId;
    }
    match /images/{userId}/{imageId} {
    	allow read, write: if request.auth != null && request.auth.uid == userId;
    }
    match /models/{userId} {
    	allow read, write: if request.auth != null && request.auth.uid == userId;
    }
    match /models/{userId}/{modelId} {
    	allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}